<Application x:Class="PhoneCamWithAndroidCam.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PhoneCamWithAndroidCam"
             StartupUri="Main/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Wpf.Common;component/Themes/Dark.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Wpf.Common;component/Common.xaml" />
                <ResourceDictionary Source="Styles/PhoneCamStyles.xaml"/>
                <ResourceDictionary Source="Styles/PipelineEditorStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
